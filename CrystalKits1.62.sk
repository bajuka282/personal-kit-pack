command /createpresetkit [<text>]:
	permission: crystalkits.admin
	trigger:
		if arg-1 is set:
			if {presetKits::*} does not contain arg-1:
				createPreset(player, arg-1)
				send "&f[%{@name}%&f]&7 Preset Created"
			else:
				send "&f[%{@name}%&f]&7 That preset already exists."


command /crystalkits [<text>] [<text>] [<text>]:
	permission: crystalkits.admin
	trigger:
		if arg-1 is "preset":
			if arg-2 is "create":
				if arg-3 is set:
					if {presetKits::*} does not contain arg-3:
						createPreset(player, arg-3)
						send "&f[%{@name}%&f]&7 Preset Created"
					else:
						send "&f[%{@name}%&f]&7 That preset already exists."
				else:
					send "&f[%{@name}%&f]&7 Specify a name."
			if arg-2 is "delete":
				deletePreset(arg-3)
				send "&f[%{@name}%&f]&7 Preset Deleted."
			else if arg-2 is not "delete" or "create":
				send "&f[%{@name}%&f]&7 Usage: /crystalkits preset [create/delete] [name]"
		else if arg-1 is not "update" or "help" or "support" or "info":
			send "&f[%{@name}%&f]&7 Unknown Argument(s)"
		else if arg-1 is "update":
			send "&f[%{@name}%&f]&7 Updates: <link:https://builtbybit.com/resources/crystal-kits.46717/>%{@yellow_color}%&nClick Here"
		else if arg-1 is "help" or "support":
			send "&f[%{@name}%&f]&7 Support Discord: <link:https://discord.gg/fMZqWf5wTN>%{@yellow_color}%&nClick Here"
		else if arg-1 is "info":
			send "&f[%{@name}%&f]&7 You are running &fCrystalKits v%{@version}% &7- &fBuild %{@build}%"

on tab complete for "/crystalkits":
	add "preset" to {_e::*}
	add "support" to {_e::*}
	add "info" to {_e::*}
	add "update" to {_e::*}
	add "create" to {_e2::*}
	add "delete" to {_e2::*}
	set tab completions for position 1 to {_e::*}
	set tab completions for position 2 to {_e2::*}


function createPreset(creator: offlineplayer, name: text):
	set {_c} to 0
	loop 36 times:
		set {Presets.slot%{_c}%::%{_name}%} to slot {_c} of {_creator}'s inventory
		add 1 to {_c}
	set {Presets.helmet::%{_name}%} to {_creator}'s helmet
	set {Presets.chestplate::%{_name}%} to {_creator}'s chestplate
	set {Presets.leggings::%{_name}%} to {_creator}'s leggings
	set {Presets.boots::%{_name}%} to {_creator}'s boots
	set {Presets.offhand::%{_name}%} to {_creator}'s offhand item
	add {_name} to {presetKits::*}
	set {presetCreator::%{_name}%} to {_creator}


function deletePreset(name: text):
	set {_c} to 0
	loop 36 times:
		delete {Presets.slot%{_c}%::%{_name}%}
		add 1 to {_c}
	delete {Presets.helmet::%{_name}%}
	delete {Presets.chestplate::%{_name}%}
	delete {Presets.leggings::%{_name}%}
	delete {Presets.boots::%{_name}%}
	delete {Presets.offhand::%{_name}%}
	remove {_name} from {presetKits::*}
	delete {presetCreator::%{_name}%}

function loadPreset(p: offlineplayer, name: text):
	set {_c} to 0
	set {_p}'s helmet to {Presets.helmet::%{_name}%}
	set {_p}'s chestplate to {Presets.chestplate::%{_name}%}
	set {_p}'s leggings to {Presets.leggings::%{_name}%}
	set {_p}'s boots to {Presets.boots::%{_name}%}
	set {_p}'s offhand item to {Presets.offhand::%{_name}%} 
	loop 36 times:
		set slot {_c} of {_p}'s inventory to {Presets.slot%{_c}%::%{_name}%}
		add 1 to {_c}
	kitNoti({_p})

function deletePresetData():
	delete {Presets.offhand::*}
	delete {Presets.boots::*}
	delete {Presets.chestplate::*}
	delete {Presets.helmet::*}
	delete {Presets.leggings::*}
	delete {presetCreator::*}
	delete {presetKits::*}
	loop 50 times:
		delete {Presets.slot%{_c}%::*}
		add 1 to {_c}
	send "&cPreset data deleted" to console



