command /totems:
	trigger:
		set {_e} to 0
		open chest inventory with 7 rows named "%{@yellow_color}%Totems" to player
		wait 1 tick
		play sound "block.nether_gold_ore.hit" with volume 100 with pitch 1 at player for player
		loop 53 times:
			set slot {_e} of player's current inventory to totem of undying
			add 1 to {_e}
		set slot 53 of player's current inventory to {head.red.back} named "%{@red_color}%Back"


on inventory click:
	if name of event-inventory is "%{@yellow_color}%Totems":
		if event-slot is 53:
			play sound "block.nether_gold_ore.hit" with volume 100 with pitch 0.7 at player for player
			cancel event
			kitRoom(player, false)
		else:
			stop





on inventory click:
	if name of event-inventory is "%{@green_color}%&lItems":
		if event-slot is 52:
			play sound "ui.toast.in" with volume 100 with pitch 1 at player for player
			cancel event
			kitRoom(player, false)
		if event-slot is 53:
			play sound "block.chest.close" with volume 100 with pitch 1 at player for player
