function kitAdmin(p: offlineplayer):
	set {_a} to 0
	open chest inventory with 3 rows named "%{@name}% &7> %{@red_color}%Admin" to {_p}
	wait 1 tick
	loop 9 times:
		set slot {_a} of {_p}'s current inventory to black stained glass pane named {@gray_color}
		add 1 to {_a}
	set {_a} to 18
	loop 9 times:
		set slot {_a} of {_p}'s current inventory to black stained glass pane named {@gray_color}
		add 1 to {_a}
	set slot 4 of {_p}'s current inventory to beacon named {@name} with lore "%{@gray_color}%Made by clapxz" and "%{@gray_color}%Version: %{@white_color}%%{@version}%" and "%{@gray_color}%Build: %{@white_color}%%{@build}%" and "" and "&8ᴄʟᴀᴘxᴢ.ᴄᴏᴍ"
	set slot 9 of {_p}'s current inventory to {head.backpack} named "%{@yellow_color}%&lChange Kit Room" with lore "%{@gray_color}%This is an up and coming feature!"
	set slot 10 of {_p}'s current inventory to blue concrete named "%{@blue_color}%&lCreate Preset" with lore "%{@gray_color}%Click to open the preset builder"
	set slot 11 of {_p}'s current inventory to red concrete named "%{@red_color}%&lDelete Preset" with lore "%{@gray_color}%Click to delete presets"
	set slot 13 of {_p}'s current inventory to chest named "%{@white_color}%&lAddons" with lore "%{@gray_color}%This is an up and coming feature!"
	set slot 17 of {_p}'s current inventory to {head.info} named "%{@yellow_color}%&lUpdates" with lore "%{@gray_color}%Click to update" and "" and "&7Requires addon %{@yellow_color}%Updater"


command /kitsave <integer>:
	trigger:
		if arg-1 is less than 10:
			editKit(player, arg-1)
			play sound "block.chest.open" with volume 100 with pitch 1 at player for player

on inventory click:
	if name of event-inventory is "%{@name}% &7- &f%{@version}%":
		if event-slot is 40:
			cancel event
			kitRoom(player, true)
		if event-slot is 42:
			cancel event
			play sound "block.iron_trapdoor.open" with volume 100 with pitch 1 at player for player
			globalKits(player, 1)
		if event-slot is 38:
			cancel event
			play sound "block.iron_trapdoor.open" with volume 100 with pitch 1 at player for player
			presetKits(player, 1)
		if event-slot is 22:
			play sound "block.ender_chest.open" with volume 100 with pitch 1 at player for player
			cancel event
			make player execute command "/enderchests"
		if event-slot is 36:
			play sound "block.shulker_box.open" with volume 100 with pitch 1 at player for player
			cancel event
			make player execute command "/kitsettings"
		if event-slot is 9:
			if click type is left mouse button:
