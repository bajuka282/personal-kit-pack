command /clearinv:
	trigger:
		clear player's inventory
		set player's offhand item to air
		set player's helmet to air
		set player's chestplate to air
		set player's leggings to air
		set player's boots to air


function kitNoti(p: offlineplayer):
	loop all players:
		if {noti::%loop-player's uuid%} is not set:
			send "%{@prefix}% %{@white_color}%%{_p}% loaded a kit." to loop-player

function roomNoti(p: offlineplayer):
	loop all players:
		if {noti::%loop-player's uuid%} is not set:
			send "%{@prefix}% %{@white_color}%%{_p}% opened the kit room." to loop-player

function ecNoti(p: offlineplayer):
	loop all players:
		if {noti::%loop-player's uuid%} is not set:
			send "%{@prefix}% %{@white_color}%%{_p}% loaded a enderchest." to loop-player

function healNoti(p: offlineplayer):
	loop all players:
		if {noti::%loop-player's uuid%} is not set:
			send "%{@prefix}% %{@white_color}%%{_p}% healed themselves." to loop-player



function editKit(p: offlineplayer, k: integer):
	set {kit.%{_k}%::%{_p}'s uuid%} to true
	open chest inventory with 7 rows named "%{@gray_color}%%{_p}%'s Kit ##%{_k}%" to {_p}
	wait 1 tick
	if {globalKits::*} contains "%{_k}%.%{_p}%":
		set {_globalKit} to "%{@green_color}%&lGLOBAL"
		set {_globalKitQuiet} to "%{@green_color}%Global"
	else:
		set {_globalKit} to "%{@red_color}%&lPRIVATE"
		set {_globalKitQuiet} to "%{@red_color}%Private"
	if {kit.%{_k}%.nickname::%{_p}'s uuid%} is not set:
		set {_nn} to "None"
	else:
		set {_nn} to {kit.%{_k}%.nickname::%{_p}'s uuid%}
	set slot 49 of {_p}'s current inventory to {head.info} named "%{@yellow_color}%&lKit Info" with lore "%{@gray_color}%Kit Number: %{@yellow_color}%%{_k}%" and "%{@gray_color}%Nickname: %{@green_color}%%{_nn}%" and "%{@gray_color}%Privacy: %{_globalKitQuiet}%"
	set {_c} to 0
	loop 36 times:
		set slot {_c} of {_p}'s current inventory to {slot%{_c}%.%{_k}%::%{_p}'s uuid%}
		add 1 to {_c}
	set slot 36 of {_p}'s current inventory to {boots.%{_k}%::%{_p}'s uuid%}
	set slot 37 of {_p}'s current inventory to {leggings.%{_k}%::%{_p}'s uuid%}
	set slot 38 of {_p}'s current inventory to {chestplate.%{_k}%::%{_p}'s uuid%}
	set slot 39 of {_p}'s current inventory to {helmet.%{_k}%.%{_k}%::%{_p}'s uuid%}
	set slot 40 of {_p}'s current inventory to {offhand.%{_k}%::%{_p}'s uuid%}
	set slot 41, 42, 43, 44 of {_p}'s current inventory to white stained glass pane named "%{@gray_color}%&l<- ARMOUR AND OFFHAND"
	set slot 48 of {_p}'s current inventory to black stained glass pane named {@gray_color}
	set slot 47 of {_p}'s current inventory to nametag named "%{@purple_color}%&lSET NICKNAME" with lore "%{@gray_color}%Click to set kit nickname."
	set slot 46 of {_p}'s current inventory to {head.bluebackpack} named "%{@blue_color}%&lIMPORT CURRENT INVENTORY" with lore "%{@gray_color}%Click to import your current inventory."
	set slot 51 of {_p}'s current inventory to {head.globe} named "%{@yellow_color}%&lPRIVACY" with lore "%{@gray_color}%Allow other users to use your kit!" and "" and "%{@gray_color}%Currently: %{_globalKit}%"
	set slot 45 of {_p}'s current inventory to {head.checkmark} named "%{@green_color}%&lSAVE AND QUIT" with lore "%{@gray_color}%Click to save and quit."
	set slot 50 of {_p}'s current inventory to black stained glass pane named {@gray_color}
	set slot 52 of {_p}'s current inventory to black stained glass pane named {@gray_color}
	set slot 53 of {_p}'s current inventory to {head.redx} named "%{@red_color}%&lQUIT" with lore "%{@gray_color}%Click to cancel, and quit."

function previewKit(p: offlineplayer, k: integer, target: offlineplayer):
	open chest inventory with 7 rows named "%{@gray_color}%Preview Kit" to {_p}
	wait 1 tick
	set {_c} to 0
	loop 36 times:
		set slot {_c} of {_p}'s current inventory to {slot%{_c}%.%{_k}%::%{_target}'s uuid%}
		add 1 to {_c}
	set slot 36 of {_p}'s current inventory to {boots.%{_k}%::%{_target}'s uuid%}
	set slot 37 of {_p}'s current inventory to {leggings.%{_k}%::%{_target}'s uuid%}
	set slot 38 of {_p}'s current inventory to {chestplate.%{_k}%::%{_target}'s uuid%}
	set slot 39 of {_p}'s current inventory to {helmet.%{_k}%.%{_k}%::%{_target}'s uuid%}
	set slot 40 of {_p}'s current inventory to {offhand.%{_k}%::%{_target}'s uuid%}
	set slot 41, 42, 43, 44 of {_p}'s current inventory to white stained glass pane named "%{@gray_color}%&l<- ARMOUR AND OFFHAND"
	set slot 48 of {_p}'s current inventory to black stained glass pane named {@gray_color}
	set slot 50 of {_p}'s current inventory to black stained glass pane named {@gray_color}
	set slot 52 of {_p}'s current inventory to black stained glass pane named {@gray_color}
	set slot 45 of {_p}'s current inventory to black stained glass pane named {@gray_color}
	set slot 46 of {_p}'s current inventory to black stained glass pane named {@gray_color}
	set slot 47 of {_p}'s current inventory to black stained glass pane named {@gray_color}
	set slot 47 of {_p}'s current inventory to black stained glass pane named {@gray_color}
	set slot 51 of {_p}'s current inventory to black stained glass pane named {@gray_color}
	set slot 49 of {_p}'s current inventory to black stained glass pane named {@gray_color}
	set slot 53 of {_p}'s current inventory to black stained glass pane named {@gray_color}
	set slot 45 of {_p}'s current inventory to red concrete named "%{@red_color}%&lBACK" with lore "%{@gray_color}%Click to return."


command {@heal_command}:
	permission: crystalkits.heal
	trigger:
		heal player
		feed player
		healNoti(player)


on inventory click:
	if name of event-inventory is "%{@gray_color}%Preview Kit":
		cancel event
		if index of event-slot is 45:
			globalKits(player, 1)
			play sound "block.dispenser.fail" with volume 5 with pitch 1 at player for player

on inventory click:
	if name of event-inventory contains "%{@gray_color}%%player%'s Kit":
		cancel event
		set {_e} to name of player's current inventory
		replace all "%{@gray_color}%%player%'s Kit ##" in {_e} with ""
		set {_kit} to {_e} parsed as an integer
		if event-slot is 47:
			if name of event-item is "%{@purple_color}%&lSET NICKNAME":
				cancel event
				nicknameKit(player, {_kit})
				play sound "ui.toast.in" with volume 100 with pitch 1 at player for player
		if event-slot is 53:
			if name of event-item is "%{@red_color}%&lQUIT":
				cancel event
				kitMain(player)
				play sound "block.chest.close" with volume 100 with pitch 1 at player for player
		if event-slot is 51:
			if name of event-item is "%{@yellow_color}%&lPRIVACY":
				cancel event
				if {globalKits::*} contains "%{_kit}%.%player%":
					remove "%{_kit}%.%player%" from {globalKits::*}
					play sound "entity.experience_orb.pickup" with volume 100 with pitch 0.8 at player for player
					set slot 51 of player's current inventory to {head.globe} named "%{@yellow_color}%&lPRIVACY" with lore "%{@gray_color}%Allow other users to use your kit!" and "" and "%{@gray_color}%Currently: %{@red_color}%&lPRIVATE"
				else:
					{globalKits::*} does not contain "%{_kit}%.%player%"
					add "%{_kit}%.%player%" to {globalKits::*}
					play sound "entity.experience_orb.pickup" with volume 100 with pitch 1.1 at player for player
					set slot 51 of player's current inventory to {head.globe} named "%{@yellow_color}%&lPRIVACY" with lore "%{@gray_color}%Allow other users to use your kit!" and "" and "%{@gray_color}%Currently: %{@green_color}%&lGLOBAL"
		if event-slot is 46:
			if name of event-item is "%{@blue_color}%&lIMPORT CURRENT INVENTORY":
				cancel event
				importInv(player)
				play sound "item.armor.equip_leather" with volume 100 with pitch 1 at player for player
		if event-slot is 45:
			if name of event-item is "%{@green_color}%&lSAVE AND QUIT":
				cancel event
				saveKit(player, {_kit})
				wait 1 tick
				kitMain(player)
				play sound "entity.experience_orb.pickup" with volume 100 with pitch 1 at player for player
				send "%{@gray_color}%Kit %{@green_color}%%{_kit}%%{@gray_color}% saved!"


function saveKit(p: offlineplayer, n: integer):
	set {_c} to 0
	loop 36 times:
		set {slot%{_c}%.%{_n}%::%{_p}'s uuid%} to slot {_c} of {_p}'s current inventory
		add 1 to {_c}
	set {helmet.%{_n}%.%{_n}%::%{_p}'s uuid%} to {_p}'s helmet
	set {chestplate.%{_n}%::%{_p}'s uuid%} to {_p}'s chestplate
	set {leggings.%{_n}%::%{_p}'s uuid%} to {_p}'s leggings
	set {boots.%{_n}%::%{_p}'s uuid%} to {_p}'s boots
	set {offhand.%{_n}%::%{_p}'s uuid%} to {_p}'s offhand item

function importInv(p: offlineplayer):
	set {_c} to 0
	loop 36 times:
		set slot {_c} of {_p}'s current inventory to slot {_c} of {_p}'s inventory
		add 1 to {_c}
	set slot 36 of {_p}'s current inventory to {_p}'s boots
	set slot 37 of {_p}'s current inventory to {_p}'s leggings
	set slot 38 of {_p}'s current inventory to {_p}'s chestplate
	set slot 39 of {_p}'s current inventory to {_p}'s helmet
	set slot 40 of {_p}'s current inventory to {_p}'s offhand item


function nicknameKit(p: offlineplayer, n: integer):
	set {nicknaming::%{_p}'s uuid%} to true
	set {nicknamingNum::%{_p}'s uuid%} to {_n}
	close {_p}'s inventory
	send "" to {_p}
	send "&7Type the nickname you want in %{@purple_color}%Chat" to {_p}
	send "" to {_p}

on chat:
	if {nicknaming::%player's uuid%} is set:
		set {kit.%{nicknamingNum::%player's uuid%}%.nickname::%player's uuid%} to message
		send "&7Set the nickname for kit %{@purple_color}%%{nicknamingNum::%player's uuid%}%&7 to %{@purple_color}%%message%"
		editKit(player, {nicknamingNum::%player's uuid%})
		delete {nicknamingNum::%player's uuid%}
		delete {nicknaming::%player's uuid%}
		play sound "ui.toast.out" with volume 100 with pitch 1 at player for player
		cancel event

		
command /kitload <integer>:
	trigger:
		if arg-1 is between 1 and 9:
			if {kit.%arg-1%::%player's uuid%} is set:
				set {_c} to 0
				set player's helmet to {helmet.%arg-1%.%arg-1%::%player's uuid%}
				set player's chestplate to {chestplate.%arg-1%::%player's uuid%}
				set player's leggings to {leggings.%arg-1%::%player's uuid%}
				set player's boots to {boots.%arg-1%::%player's uuid%}
				set player's offhand item to {offhand.%arg-1%::%player's uuid%}
				set {lastkit::%player's uuid%} to arg-1
				add 1 to {kitsloaded::%player's uuid%}
				loop 36 times:
					set slot {_c} of player's inventory to {slot%{_c}%.%arg-1%::%player's uuid%}
					add 1 to {_c}
				kitNoti(player)
			else:
				send "&4%{@red_color}%This kit does not exist"


command /stealkit <offlineplayer> <integer>:
	permission: steal.kit
	trigger:
		if arg-2 is between 1 and 9:
			if {kit.%arg-2%::%arg-1's uuid%} is set:
				set {_c} to 0
				set player's helmet to {helmet.%arg-2%.%arg-2%::%arg-1's uuid%}
				set player's chestplate to {chestplate.%arg-2%::%arg-1's uuid%}
				set player's leggings to {leggings.%arg-2%::%arg-1's uuid%}
				set player's boots to {boots.%arg-2%::%arg-1's uuid%}
				set player's offhand item to {offhand.%arg-2%::%arg-1's uuid%}
				add 1 to {kitsloaded::%player's uuid%}
				loop 36 times:
					set slot {_c} of player's inventory to {slot%{_c}%.%arg-2%::%arg-1's uuid%}
					add 1 to {_c}
				send "%{@green_color}%%arg-1%'s Kit %{@green_color}%&l%arg-2% <##12FB00>loaded"
				kitNoti(player)
			else:
				send "&4%{@red_color}%This kit does not exist"

function stealKit(p: offlineplayer, target: offlineplayer, kit: integer):
	set {_c} to 0
	set {_p}'s helmet to {helmet.%{_kit}%.%{_kit}%::%{_target}'s uuid%}
	set {_p}'s chestplate to {chestplate.%{_kit}%::%{_target}'s uuid%}
	set {_p}'s leggings to {leggings.%{_kit}%::%{_target}'s uuid%}
	set {_p}'s boots to {boots.%{_kit}%::%{_target}'s uuid%}
	set {_p}'s offhand item to {offhand.%{_kit}%::%{_target}'s uuid%}
	add 1 to {kitsloaded::%{_p}'s uuid%}
	loop 36 times:
		set slot {_c} of {_p}'s inventory to {slot%{_c}%.%{_kit}%::%{_target}'s uuid%}
		add 1 to {_c}
	kitNoti({_p})
				
				
				
on respawn:
	if {autokit::%player's uuid%} is not set:
		if {lastkit::%player's uuid%} is set:
			wait 1 second
			make player execute command "/k%{lastkit::%player's uuid%}%"
		else:
			stop

on join:
	if {autokit::%player's uuid%} is not set:
		if {lastkit::%player's uuid%} is set:
			wait 1 second
			make player execute command "/k%{lastkit::%player's uuid%}%"
		else:
			stop

command /kits:
	aliases: kit, k, kitmenu
	trigger:
		kitMain(player)

command /kit:
	aliases: kit, k, kitmenu
	trigger:
		kitMain(player)


function kitMain(p: offlineplayer):
	set {_a} to 0
	open chest inventory with 5 rows named "%{@name}% &7- &f%{@version}%" to {_p}
	wait 1 tick
	loop 9 times:
		set slot {_a} of {_p}'s current inventory to black stained glass pane named {@gray_color}
		add 1 to {_a}
	set slot 9 of {_p}'s current inventory to chest named "%{@yellow_color}%Kit 1" with lore "%{@gray_color}%Left click to load Kit 1" and "%{@gray_color}%Right click to save Kit 1" and "" and "&8Opens kit editing menu"
	set slot 10 of {_p}'s current inventory to chest named "%{@yellow_color}%Kit 2" with lore "%{@gray_color}%Left click to load Kit 2" and "%{@gray_color}%Right click to save Kit 2" and "" and "&8Opens kit editing menu"
