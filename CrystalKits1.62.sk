	set slot 12 of {_p}'s current inventory to chest named "%{@yellow_color}%Kit 4" with lore "%{@gray_color}%Left click to load Kit 4" and "%{@gray_color}%Right click to save Kit 4" and "" and "&8Opens kit editing menu"
