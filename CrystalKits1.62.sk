function importEC(p: offlineplayer):
	set {_c} to 0
	loop 27 times:
		set slot {_c} of {_p}'s current inventory to slot {_c} of {_p}'s ender chest 
		add 1 to {_c}

function saveEC(p: offlineplayer, n: integer):
	set {_c} to 0
	loop 27 times:
		set {ecslot%{_c}%.%{_n}%::%{_p}'s uuid%} to slot {_c} of {_p}'s current inventory
		add 1 to {_c}

function loadEC(p: offlineplayer, n: integer):
	set {_c} to 0
	loop 27 times:
		set slot {_c} of {_p}'s ender chest  to {ecslot%{_c}%.%{_n}%::%{_p}'s uuid%}
		add 1 to {_c}
	ecNoti({_p})


function editEC(p: offlineplayer, n: integer):
	open chest inventory with 4 rows named "<##7289DA>&lEnderchest Edit - %{_n}%" to {_p}
	wait 1 tick
	set {_c} to 0
	loop 27 times:
		set slot {_c} of {_p}'s current inventory to {ecslot%{_c}%.%{_n}%::%{_p}'s uuid%}
		add 1 to {_c}
	set slot 34 of {_p}'s current inventory to ender chest named "<##7289DA>&lIMPORT CURRENT ENDERCHEST"
	set slot 27 of {_p}'s current inventory to red wool named "%{@red_color}%&lCANCEL"
	set slot 35 of {_p}'s current inventory to lime wool named "%{@green_color}%&lSAVE AND QUIT"

function globalKits(p: offlineplayer, page: number):
	set {_slot} to 0
	set {_pageVis} to size of {globalKits::*}
	set {_pageVis} to {_pageVis} / 45
	if {_pageVis} is less than or equal to 0:
		add 1 to {_pageVis}
	set {_pageVis} to ceil({_pageVis})
	set {_maxPage} to ceil({_pageVis})
	set {_pageVis} to "%{_page}%/%{_pageVis}%"
	open chest inventory with 10 rows named "%{@pink_color}%&lGlobal Kits%{@white_color}% &7(%{_pageVis}%)" to {_p}
	wait 1 tick
	set {_pageStart} to 1 + 45 * ({_page} - 1)
	loop 45 times:
		set {_id} to {globalKits::%{_pageStart}%}
		delete {_parts::*}
		set {_parts::*} to "%{_id}%" split at "."
		set {_Player} to {_parts::2} parsed as an offlineplayer
		set {_Kit} to {_parts::1} parsed as an integer
		if {_slot} < 45:
			if {_id} is set:
				if {kit.%{_Kit}%.nickname::%{_Player}'s uuid%} is not set:
					set {_nickname} to "None"
				else:
					set {_nickname} to {kit.%{_Kit}%.nickname::%{_Player}'s uuid%}
				set slot {_slot} of {_p}'s current inventory to skull of {_Player} named "%{@gray_color}%&f%{_Player}%&7's &fKit&7 &f##%{_Kit}%" with lore "&7Kit Nickname: <##7289DA>%{_nickname}%" and "" and "%{@gray_color}%➥ %{@green_color}%Left Click&7 to load" and "%{@gray_color}%➥ <##FF0000>Right Click&7 to preview" and "" and "&8Player: %{_Player}%"  and "&8Kit: %{_Kit}%"
				add 1 to {_slot}
				add 1 to {_pageStart}
			else:
				add 1 to {_pageStart}
		else:
			exit loop
	if {_page} is less than {_maxPage}:
		set slot 53 of {_p}'s current inventory to arrow named "%{@gray_color}%ɴᴇxᴛ ᴘᴀɢᴇ" with lore "%{@gray_color}%Click to navigate."
	else:
		set slot 53 of {_p}'s current inventory to black stained glass pane named {@gray_color}
	if {_page} is not 1:
		set slot 45 of {_p}'s current inventory to arrow named "%{@gray_color}%ᴘʀᴇᴠɪᴏᴜs ᴘᴀɢᴇ" with lore "%{@gray_color}%Click to navigate."
	else:
		set slot 45 of {_p}'s current inventory to black stained glass pane named {@gray_color}
	set slot 46,47,48,49,50,51,52 of {_p}'s current inventory to black stained glass pane named {@gray_color}
	set slot 49 of {_p}'s current inventory to {head.red.back} named "<##FF0000>&lBACK" with lore "%{@gray_color}%Click to return."

on inventory click:
	if name of player's current inventory contains "&lGlobal Kits":
		cancel event
		event-inventory is not player's inventory
		set {_e} to name of player's current inventory
		replace all "%{@pink_color}%&lGlobal Kits%{@white_color}% &7" in {_e} with ""
		replace all ")" in {_e} with ""
		replace all "(" in {_e} with ""
		set {_parts::*} to {_e} split at "/"
		set {_e} to {_parts::1}
		set {_a} to {_e}
		if index of event-slot is 53:
			event-slot is an arrow
			set {_a} to {_a} parsed as an integer
			set {_an} to {_a} + 1
			globalKits(player, {_an})
			play sound "item.book.page_turn" at volume 100 at player for player
		if index of event-slot is 45:
			event-slot is an arrow
			set {_a} to {_a} parsed as an integer
			set {_an2} to {_a} - 1
			if {_an2} is more than 0:
				globalKits(player, {_an2})
				play sound "item.book.page_turn" at volume 100 at player for player
			else:
				stop
		else:
			if event-slot is a player head:
				event-slot is not {head.red.back}
				event-slot is not {head.globe}
				play sound "block.dispenser.fail" with volume 5 with pitch 1 at player for player
				if click type is left mouse button:
					set {_e} to line 6 of event-slot's lore
					replace all "&8Player: " in {_e} with ""
					set {_e} to {_e} parsed as an offlineplayer
					set {_kit} to line 7 of event-slot's lore
					replace all "&8Kit: " in {_kit} with ""
					set {_kit} to {_kit} parsed as an integer
					stealKit(player, {_e}, {_kit})
				if click type is right mouse button:
					set {_e} to line 6 of event-slot's lore
					replace all "&8Player: " in {_e} with ""
					set {_e} to {_e} parsed as an offlineplayer
					set {_kit} to line 7 of event-slot's lore
					replace all "&8Kit: " in {_kit} with ""
					set {_kit} to {_kit} parsed as an integer
					previewKit(player, {_kit}, {_e})
				else:
					cancel event
		if index of event-slot is 49:
			kitMain(player)
			play sound "block.iron_trapdoor.close" with volume 100 with pitch 1 at player for player
	

