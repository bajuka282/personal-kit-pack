	set slot 16 of {_p}'s current inventory to chest named "%{@yellow_color}%Kit 8" with lore "%{@gray_color}%Left click to load Kit 8" and "%{@gray_color}%Right click to save Kit 8" and "" and "&8Opens kit editing menu"
