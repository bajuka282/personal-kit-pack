options:
	prefix: "<##FFFFFF>[<##008AFB>&lC<##FF0000>&lK&l<##FFFFFF>]"
	name: "<##008AFB>&lCrystal<##FF0000>&lKits"
	version: "1.62"
	build: "Public"


	# Colors (Use two ## for correct usage) (&7, or &e types can be used)
	# Most colors are here.
	gray_color: "<##8C8C8C>"
	yellow_color: "<##FBF300>"
	red_color: "<##FB1717>"
	purple_color: "<##AF00FF>"
	green_color: "<##12FB00>"
	pink_color: "<##FB00C2>"
	blue_color: "<##00A2FF>"
	white_color: "<##FFFFFF>"

	# Commands

	heal_command: /heal

	# Permissions:

	# crystalkits.admin - /crystalkits
	# crystalkits.heal - /heal



